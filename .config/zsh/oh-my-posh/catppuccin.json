{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "os": "#ACB0BE",
    "closer": "p:os",
    "pink": "#F5C2E7",
    "muave": "#CBA6F7",
    "blue": "#89B4FA",
    "weight": "#FFFFFF",
    "text": "#494D64",
    "peach": "#FAB387",
    "yellow": "#F9E2AF",
    "flamingo": "#F2CDCD",
    "rosewater": "#F5E0DC",
    "red": "#F38BA8",
    "crust": "#11111B"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "plain",
          "foreground": "p:os",
          "template": "\n{{.Icon}} "
        },
        {
          "type": "session",
          "style": "plain",
          "foreground": "p:blue",
          "template": "{{ .UserName }} "
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "p:pink",
          "properties": {
            "home_icon": "~",
            "style": "agnoster_full"
          },
          "template": "{{ .Path }} "
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "p:muave",
          "properties": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": true,
            "fetch_upstream_icon": false,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "template": "{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}"
        }
      ]
    },
    {
      "type": "rprompt",
      "overflow": "hide",
      "segments": [
        {
          "type": "executiontime",
          "style": "plain",
          "powerline_symbol": "",
          "foreground": "p:peach",
          "background": "transparent",
          "template": " {{ .FormattedMs }} ",
          "properties": {
            "threshold": 200,
            "style": "austin",
            "always_enabled": true
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "template": "❯",
          "background": "transparent",
          "foreground_templates": [
            "{{if gt .Code 0}}p:red{{end}}",
            "{{if eq .Code 0}}p:blue{{end}}"
          ]
        }
      ]
    }
  ],
  "transient_prompt": {
    "background": "transparent",
    "template": "\n❯ ",
    "foreground_templates": [
      "{{if gt .Code 0}}p:red{{end}}",
      "{{if eq .Code 0}}p:muave{{end}}"
    ]
  },
  "secondary_prompt": {
    "foreground": "p:blue",
    "background": "transparent",
    "template": "❯❯ "
  },
  "final_space": true,
  "version": 2
}
